{
  "compilerOptions": {
    "target": "ES2022",
    "module": "ESNext",
    "moduleResolution": "bundler",
    "allowJs": true,
    "checkJs": true,
    "noEmit": true,
    "strict": false,
    "skipLibCheck": true,
    "types": ["chrome-types"],
    "typeRoots": ["./node_modules", "../../node_modules"]
  },
  "include": ["*.js"]
}
